      "IncludeFormattedMessage": true,
      "IncludeScopes": true,
      "ParseStateValues": true
    }
  },
  "ConnectionStrings": {
    "Database": ""
  },
  "Database": {
    "Name": "Vanguard",
    "DataStoreType": "main"
  },
  "ServiceName": "Vanguard Payment Service",
  "Otlp": {
    "Endpoint": "http://localhost:4317",
    "ApiKey": ""
  },
  "AspNetCoreInstrumentation": {
    "RecordException": "true"
  }
}
